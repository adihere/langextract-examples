{
    "extraction_config": {
        "prompt": "Extract following information only - ISIN, Asset Class, Total Expense Ratio (TER), and Issuing Company.\n Extract exact text for these fields. Do not paraphrase. Include any relevant contextual information in attributes.\nLabel fields as:\n- isin: ISIN code\n- asset_class: Type of asset or investment\n- expense_ratio: Total Expense Ratio (TER)\n- issuer: Fund issuing company \n Do not output fields that are not supported by the schema above \n ",
        "max_workers": 2,
        "extraction_passes": 3,
        "chunk_overlap": 200,
        "model_settings": {
            "model_id": "mistral-small-latest",
            "base_url": "https://api.mistral.ai/v1/conversations",
            "fence_output": true,
            "use_schema_constraints": false
        },
        "examples": [
            {
                "text": "GBP (Dist) Share Class \n ISIN: IE00B00FV128 Exchange Traded Fund (ETF)  \n Manager: BlackRock Asset Management Ireland Limited",
                "extractions": [
                    {
                        "extraction_class": "isin",
                        "extraction_text": "IE00B4ND3602",
                        "attributes": {
                            "type": "ETC"
                        }
                    },
                    {
                        "extraction_class": "asset_class",
                        "extraction_text": "Commodities - Precious Metals",
                        "attributes": {
                            "specific": "Gold"
                        }
                    },
                    {
                        "extraction_class": "expense_ratio",
                        "extraction_text": "0.12% per annum",
                        "attributes": {
                            "type": "TER"
                        }
                    },
                    {
                        "extraction_class": "issuer",
                        "extraction_text": "BlackRock Asset Management",
                        "attributes": {
                            "brand": "iShares"
                        }
                    }
                ]
            }
        ],
        "model_id": "mistral-small-latest",
        "max_chunk_size": 8000,
        "output_path": "fund_extractions.jsonl"
    }
}
